<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #CCCCCC;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="forms.RegController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="60.0" layoutY="65.0" prefHeight="57.0" prefWidth="481.0" text="Welcome to Autopark!" textAlignment="CENTER" textOverrun="CLIP" wrapText="true">
         <font>
            <Font name="Century Gothic" size="43.0" />
         </font>
      </Label>
      <GridPane layoutX="60.0" layoutY="175.0" prefHeight="94.0" prefWidth="481.0" style="-fx-background-color: #CCCCCC;">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="236.0" minWidth="10.0" prefWidth="168.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="313.0" minWidth="10.0" prefWidth="313.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" text="Username">
               <font>
                  <Font name="Century Gothic" size="20.0" />
               </font>
            </Label>
            <Label text="Password" GridPane.rowIndex="1">
               <font>
                  <Font name="Century Gothic" size="20.0" />
               </font>
            </Label>
            <TextField fx:id="unametf" prefHeight="38.0" prefWidth="313.0" GridPane.columnIndex="1">
               <font>
                  <Font name="Century Gothic" size="20.0" />
               </font>
            </TextField>
            <PasswordField fx:id="pwdtf" prefHeight="38.0" prefWidth="313.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <font>
                  <Font name="Century Gothic" size="20.0" />
               </font>
            </PasswordField>
         </children>
      </GridPane>
      <Button fx:id="loginbutton" layoutX="60.0" layoutY="319.0" mnemonicParsing="false" onAction="#onLoginButtonClick" prefHeight="31.0" prefWidth="231.0" text="Log in" />
      <Button fx:id="regbutton" layoutX="310.0" layoutY="319.0" mnemonicParsing="false" onAction="#onRegButtonClick" prefHeight="31.0" prefWidth="231.0" text="Register" />
      <GridPane layoutX="60.0" layoutY="274.0" prefHeight="39.0" prefWidth="481.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="logerrlabel" textFill="RED" GridPane.halignment="RIGHT" />
         </children>
      </GridPane>
   </children>
   <effect>
      <Blend />
   </effect>
</AnchorPane>
